(= (respond $message)
    (let $_ (add-langchain-history $message)
        (
            (let $ans (py-dot  ((py-dot (get-langchain-model)  invoke) (get-langchain-history)) content)
                  (let $_ (add-langchain-history (assistant $ans))
                   $ans
                  )
            )
        )
    )
)


!(Response (respond (messages)))





